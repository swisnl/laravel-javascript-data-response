{
    "name": "swisnl/laravel-javascript-data-response",
    "type": "library",
    "description": "JavaScript data response macro for Laravel",
    "keywords": [
        "swisnl",
        "laravel-javascript-data-response"
    ],
    "homepage": "https://github.com/swisnl/laravel-javascript-data-response",
    "license": "MIT",
    "authors": [
        {
            "name": "Jasper Zonneveld",
            "email": "jasper@swis.nl",
            "homepage": "https://www.swis.nl/over-ons/jasper-zonneveld",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit" : "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Swis\\Laravel\\JavaScriptData\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swis\\Laravel\\JavaScriptData\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "php-cs-fixer fix --dry-run -v",
        "fix-style": "php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    }
}
